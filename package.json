{
  "name": "app",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm-run-all build:server build:assets",
    "build:server": "esbuild src/init-app.ts --bundle --platform=node --outfile=build/index.js --format=esm --packages=external",
    "build:assets": "node ./copy-assets.js",
    "start": "node ./build/index.js",
    "dev": "npm-run-all --parallel watch:express watch:sass",
    "test": "jest",
    "lint": "eslint -c eslint.config.mjs ",
    "watch:express": "npx nodemon --exec tsx src/init-app.ts",
    "watch:sass": "sass --watch ./src/sass/main.scss ./src/public/style.css "
  },
  "type": "module",
  "author": "etan675",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "esbuild": "^0.24.2",
    "eslint": "^9.16.0",
    "eslint-plugin-jest": "^28.9.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sass": "^1.83.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "dependencies": {
    "@types/jest": "^29.5.14",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "npm-run-all": "^4.1.5",
    "pg": "^8.13.1"
  }
}
